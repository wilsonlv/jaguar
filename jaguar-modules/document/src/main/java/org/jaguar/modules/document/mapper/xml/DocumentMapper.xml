<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jaguar.modules.document.mapper.DocumentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseResultMap" type="org.jaguar.modules.document.model.Document">
        <id property="id" column="id_"/>
        <result property="originalName" column="original_name"/>
        <result property="extension" column="extension_"/>
        <result property="absolutePath" column="absolute_path"/>
        <result property="totalSpace" column="total_space"/>
        <result property="deleted" column="deleted_"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>


</mapper>
