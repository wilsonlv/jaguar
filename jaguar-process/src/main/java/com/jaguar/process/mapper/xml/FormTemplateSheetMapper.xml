<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaguar.process.mapper.FormTemplateSheetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jaguar.process.model.po.FormTemplateSheet">
        <result column="form_template_id" property="formTemplateId"/>
        <result column="element_id" property="elementId"/>
        <result column="name_" property="name"/>
        <result column="sort_no" property="sortNo"/>
    </resultMap>


    <select id="selectIdPage" resultType="java.lang.Long">
        select id_ from form_template_sheet
        <where>
            <if test="cm.formTemplateId != null and cm.formTemplateId != ''">
                AND form_template_id = #{cm.formTemplateId}
            </if>
            <if test="cm.elementId != null and cm.elementId != ''">
                AND element_id = #{cm.elementId}
            </if>
            <if test="cm.name != null and cm.name != ''">
                AND name_ = #{cm.name}
            </if>
            <if test="cm.sortNo != null and cm.sortNo != ''">
                AND sort_no = #{cm.sortNo}
            </if>
            <if test="cm.deleted != null">
                AND deleted_ = #{cm.deleted}
            </if>
        </where>
    </select>
</mapper>
